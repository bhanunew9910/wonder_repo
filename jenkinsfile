pipeline{
     
    agent any
         
    stages {
         
        stage("Build"){
            steps {
                echo "this is first application building"
            }
         }
         stage("Test") {
             steps {
                 echo 'the application is testing'
             }
         }
         stege("deploy") {
             steps {
                 echo 'the application is deploy'
             }
         }
         
     }
}
